{
  "name": "firetruss",
  "version": "7.7.0",
  "description": "Advanced data sync layer for Firebase and Vue.js",
  "scripts": {
    "update": "yarn upgrade-interactive --latest",
    "build": "yarn run grunt",
    "lint": "eslint src/**/*.js",
    "test": "ava",
    "finalize": "yarn run grunt replace default --release=\"$(jq -r .version package.json)\""
  },
  "repository": "https://github.com/Reviewable/firetruss",
  "keywords": [
    "firebase",
    "vuejs",
    "model",
    "worker",
    "angular"
  ],
  "author": "Piotr Kaminski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Reviewable/firetruss/issues"
  },
  "homepage": "https://github.com/Reviewable/firetruss",
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@rollup/plugin-commonjs": "25.x",
    "@rollup/plugin-node-resolve": "15.x",
    "ava": "4.x",
    "compose-source-map": "^1.0.0",
    "globals": "^15.9.0",
    "grunt": "^1.0.0",
    "grunt-contrib-clean": "2",
    "grunt-contrib-uglify": "5",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "25.x",
    "grunt-git": "^1.1.1",
    "grunt-rollup": "^12.0.0",
    "grunt-text-replace": "^0.4.0",
    "load-grunt-tasks": "5",
    "reviewable-configs": "Reviewable/reviewable-configs",
    "rollup": "4.x",
    "testdouble": "3.x"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "main": "dist/firetruss.umd.js",
  "module": "dist/firetruss.es2015.js",
  "types": "types/index.d.ts",
  "dependencies": {
    "lodash": "4",
    "vue": "^2.1.6"
  },
  "packageManager": "yarn@1.22.22"
}
