{
  "name": "firetruss",
  "version": "7.2.0",
  "description": "Advanced data sync layer for Firebase and Vue.js",
  "scripts": {
    "setup": "rm -rf node_modules; npm install;",
    "update": "yarn upgrade-interactive --latest",
    "test": "ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pkaminski/firetruss.git"
  },
  "keywords": [
    "firebase",
    "vuejs",
    "model",
    "worker",
    "angular"
  ],
  "author": "Piotr Kaminski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pkaminski/firetruss/issues"
  },
  "homepage": "https://github.com/pkaminski/firetruss",
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@rollup/plugin-commonjs": "25.x",
    "@rollup/plugin-node-resolve": "15.x",
    "ava": "4.x",
    "compose-source-map": "^1.0.0",
    "globals": "^15.9.0",
    "grunt": "^1.0.0",
    "grunt-contrib-clean": "2",
    "grunt-contrib-uglify": "5",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "25.x",
    "grunt-exec": "^3.0.0",
    "grunt-git": "^1.0.1",
    "grunt-release": "reviewable/grunt-release#master",
    "grunt-rollup": "^12.0.0",
    "grunt-text-replace": "^0.4.0",
    "load-grunt-tasks": "5",
    "reviewable-configs": "Reviewable/reviewable-configs",
    "rollup": "4.x",
    "testdouble": "3"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "main": "dist/firetruss.umd.js",
  "module": "dist/firetruss.es2015.js",
  "types": "types/index.d.ts",
  "dependencies": {
    "lodash": "4",
    "performance-now": "2",
    "vue": "^2.1.6"
  }
}
